var MathData1 = function (context) {
  var ui = $.summernote.ui;

  // create dropdown
  var event1 = ui.buttonGroup([
    ui.button({
      contents: 'Math 1 <i class="fa fa-caret-down" aria-hidden="true"></i>',
      tooltip: 'Math Icons',
      data: {
        toggle: 'dropdown'
      }
    }),
    ui.dropdown({
      items: [
        '&#402;', '&#915;', '&#916;', '&#928;', '&#931;', '&#932;', '&#933;', '&#934;', '&#935;', '&#936;', '&#937;', '&#945;', '&#946;', '&#947;', '&#948;', '&#949;', '&#950;', '&#951;', '&#952;', '&#955;', '&#956;', '&#957;'
      ],
      callback: function (items) {
        $(items).find('li a').on('click', function(){
          context.invoke("editor.pasteHTML", $(this).html())
        })
      }
    })
  ]);

  return event1.render();   // return button as jquery object
};


