.l-section-small.is-bg-asphalt
  .l-wrap
    .container-fluid
      .row.l-margin-bottom
        .col-md-12.text-center
          %h1.l-margin-bottom.text-center.h-separator.h-separator-center{style: 'letter-spacing:1.5px;'}
            Management Console

      .row.l-margin-top.l-margin-bottom
        .col-sm-12
          .box-item

            -#Trial User Numbers
            .col-md-4.col-sm-6.col-xs-12
              .stats-box
                .stats-box-inner
                  .h3
                    =@trial_students.count
                  .h6
                    Trial Users
            .col-md-4.col-sm-6.col-xs-12
              .stats-box
                .stats-box-inner
                  .h3
                    =@valid_trial_students.count
                  .h6
                    Valid Trials
            .col-md-4.col-sm-6.col-xs-12
              .stats-box
                .stats-box-inner
                  .h3
                    =@invalid_trial_students.count
                  .h6
                    Expired Trials


            -#Subscription User Numbers
            .col-md-4.col-sm-6.col-xs-12
              .stats-box
                .stats-box-inner
                  .h3
                    =@sub_students.count
                  .h6
                    Sub Users

            .col-md-4.col-sm-6.col-xs-12
              .stats-box
                .stats-box-inner
                  .h3
                    =@valid_sub_students.count
                  .h6
                    Valid Sub Users

            .col-md-4.col-sm-6.col-xs-12
              .stats-box
                .stats-box-inner
                  .h3
                    =@invalid_sub_students.count
                  .h6
                    Invalid Sub Users

            .col-md-4.col-sm-6.col-xs-12
              .stats-box
                .stats-box-inner
                  .h3
                    =@subscriptions.count
                  .h6
                    Subscriptions Total

            .col-md-4.col-sm-6.col-xs-12
              .stats-box
                .stats-box-inner
                  .h3
                    =@active_subs.count
                  .h6
                    Active Subs

            .col-md-4.col-sm-6.col-xs-12
              .stats-box
                .stats-box-inner
                  .h3
                    =@cancelled_subs.count
                  .h6
                    Canceled Subs

            .col-md-4.col-sm-6.col-xs-12
              .stats-box
                .stats-box-inner
                  .h3
                    =@past_due_subs.count
                  .h6
                    Past Due Subs

            .col-md-4.col-sm-6.col-xs-12
              .stats-box
                .stats-box-inner
                  .h3
                    =@cancel_pending_subs.count
                  .h6
                    Cancel Pending Subs


            .col-md-4.col-sm-6.col-xs-12
              .stats-box
                .stats-box-inner
                  .h3
                    ='-'
                  .h6
                    ='-'



            .col-md-4.col-sm-6.col-xs-12
              .stats-box
                .stats-box-inner
                  .h3
                    =@total_enrollments.count
                  .h6
                    Enrolments - Total

            .col-md-4.col-sm-6.col-xs-12
              .stats-box
                .stats-box-inner
                  .h3
                    =@active_enrollments.count
                  .h6
                    Enrolments - Active

            .col-md-4.col-sm-6.col-xs-12
              .stats-box
                .stats-box-inner
                  .h3
                    =@expired_enrollments.count
                  .h6
                    Enrolments - Expired

