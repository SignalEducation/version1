.col-sm-6.col-sm-offset-3.text-center.l-margin-bottom-big
  -if current_corporate
    -if current_corporate.logo.url
      =link_to root_url do
        =image_tag current_corporate.logo.url, class: 'md-image'
    -else
      =link_to root_url do
        =image_tag 'ls-logo-red.svg', class: 'md-image'
  -else
    =link_to root_url do
      =image_tag 'ls-logo-red.svg', class: 'lg-image'
