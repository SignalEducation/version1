-if @second_element && File.exists?(Rails.root.join('app', 'views', 'home_pages', "_#{@second_element}.html.haml"))
  =render partial: "home_pages/#{@second_element}"
-elsif @first_element && File.exists?(Rails.root.join('app', 'views', 'home_pages', "_#{@first_element}.html.haml"))
  =render partial: "home_pages/#{@first_element}"
-else
  =render partial: 'home_pages/default'

