%main
  %article.bg-white
    -if flash[:success] || flash[:error] || flash[:warning]
      =render partial: 'layouts/flash_messages'
    .container
      %header.hero-section.p-0.mb-4
        %div
          %svg{:fill => "none", :height => "240", :viewbox => "0 0 241 240", :width => "241", :xmlns => "http://www.w3.org/2000/svg", "xmlns:xlink" => "http://www.w3.org/1999/xlink"}
            %rect{:fill => "url(#pattern0)", :height => "240", :width => "240", :x => "0.5"}
            %defs
              %pattern#pattern0{:height => "1", :patterncontentunits => "objectBoundingBox", :width => "1"}
                %use{:transform => "scale(0.0015625)", "xlink:href" => "#image0"}
              %image#image0{:height => "640", :width => "640", "xlink:href" => "data:image/png;base64,"}
        %h1.h2-hero.mb-2
          =t('views.subscriptions.upgrade_complete.h1')

      %section.pb-md-6.pb-5
        .row.justify-content-center
          .col-xs-12.col-sm-8
            .box-item.text-center.py-4.mx-auto{style: 'max-width: 640px;'}
              %h5.mb-1
                ="You have subscribed to the #{t("views.general.subscription_label_in_months.a#{@subscription.subscription_plan.payment_frequency_in_months}")}."
                %p
                  ="Your payment receipt will be sent to #{current_user.email}"
                =link_to library_special_link(@subscription&.exam_body&.group) do
                  .btn.btn-primary
                    ='Continue Learning'

:javascript
  $(document).ready(function(){
    window.uetq = window.uetq || [];
    window.uetq.push('event', 'Sub Purchases', {
      'event_category': "#{@subscription.subscription_plan.exam_body.name}",
      'event_label': "#{@subscription.subscription_plan.name}",
      'event_value': "#{@subscription.subscription_plan.price}"
    });

    fbq('track', 'Sub Purchases', {
      value: "#{@subscription&.subscription_plan&.price}",
      currency: "#{@subscription&.subscription_plan&.currency&.iso_code}",
      subscription_id: "#{@subscription&.id}",
      name: "#{@subscription&.subscription_plan&.name}",
      brand: "#{@subscription&.subscription_plan&.exam_body&.name}",
      subscription_interval: "#{@subscription&.subscription_plan&.interval_name}",
      content_category: "#{@subscription.kind.humanize}"
    });

    if ("#{@subscription.kind}" === 'new_subscription') {
      var price = "#{@discounted_price}"

      gtag('event', 'Sub Purchases', {
        "transaction_id": "#{@subscription&.id}",
        "affiliation": "subscription",
        "value": "#{@subscription&.subscription_plan&.price}",
        "currency": "#{@subscription&.subscription_plan&.currency&.iso_code}",
        "items": [
          {
            "id": "#{@subscription&.subscription_plan&.id}",
            "name": "#{@subscription&.subscription_plan&.name}",
            "brand": "Subscription",
            "category": "#{@subscription&.subscription_plan&.interval_name}",
            "quantity": 1,
            "price": price
          },
        ]
      });
    }

    let segmentProps = {      
      email: "#{current_user&.email}",      
      userId: "#{current_user&.email}",                
      isLoggedIn: "#{current_user&.email}",
      sessionId: "#{session&.id}",
      name: 'APP : Payment Complete',
      preferredExamBodyId: "#{current_user&.preferred_exam_body_id}",
      preferredExamBody: "#{current_user&.preferred_exam_body&.name}",
      programName: "#{@subscription&.subscription_plan&.exam_body&.name}",
      planName: "#{@subscription&.subscription_plan&.name}",
      planPrice: "#{@subscription&.subscription_plan&.price}",
      planType: "#{@subscription&.subscription_plan&.interval_name&.downcase}",
      discountedPrice: "#{@discounted_price}", 
      discountCode: "#{@coupon_code}",
      paymentType: "#{@subscription&.kind}",
      subscriptionType: "Subscription",
      paymentProviderType: "#{@subscription&.stripe_status ? 'Stripe' : 'Paypal'}"
    };

    let segmentData = createBasePropertiesSegment(segmentProps);

    analytics.page('Page Views', segmentData.name, segmentData);
  });
